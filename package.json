{
  "name": "rai-dashboard",
  "version": "1.0.0",
  "description": "Responsible AI Dashboard - Static embeddable component",
  "type": "module",
  "main": "./dist/rai-dashboard.umd.cjs",
  "module": "./dist/rai-dashboard.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/rai-dashboard.js",
      "require": "./dist/rai-dashboard.umd.cjs"
    },
    "./style.css": "./dist/rai-dashboard.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:static": "tsc -b && BUILD_MODE=static vite build",
    "preview": "vite preview",
    "preview:static": "BUILD_MODE=static vite build && vite preview",
    "lint": "eslint ."
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^6.5.0",
    "@mui/material": "^6.5.0",
    "lucide-react": "^0.544.0",
    "react-router-dom": "^6.28.0",
    "recharts": "^2.15.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/vite": "^4.1.11",
    "@types/node": "^25.0.3",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.1.0",
    "vite-plugin-dts": "^4.5.0"
  },
  "keywords": [
    "responsible-ai",
    "rai",
    "dashboard",
    "react",
    "component"
  ],
  "license": "MIT"
}
